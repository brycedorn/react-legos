    "@storybook/addon-notes": "^3.2.17",
    "@storybook/addon-options": "^3.2.17",
    "@storybook/react": "^3.2.17",
    "@storybook/storybook-deployer": "^2.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^5.8.35",
    "babel-loader": "^5.4.0",
